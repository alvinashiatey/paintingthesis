---
{% extends "layouts/base.njk" %} 
{% block content %}
<canvas id="c2"></canvas>
<div class="info">
    <div class="info__header">
        <div class="vm__identity">
            <a href="/"> <h1>Vibrant Matters</h1> </a>
        </div>
    </div>
    <div class="info__main">
        <div class="info__container">
            <div class="info__info">
                <h1>Press Release</h1>
            </div>
            <div class="article__wrapper">
                <article>
                    {{ content | safe }}
                </article>
            </div>
        </div>
    </div>
</div>
{% endblock %}